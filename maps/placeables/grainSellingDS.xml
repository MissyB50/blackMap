 </hotspots>

  <triggerMarkers>
  <triggerMarker node="unloadTriggerGrainMarker"    filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
  <triggerMarker node="woodActivationTriggerMarker"		filename="$data/shared/assets/marker/markerIcons.xml" id="EXCLAMATION_MARK" adjustToGround="false" />
 </triggerMarkers>

